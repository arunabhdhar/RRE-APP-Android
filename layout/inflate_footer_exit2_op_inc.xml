<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@color/blue"
    android:padding="10dp"
    android:layout_height="wrap_content">

    <View
        android:layout_width="match_parent"
        android:background="@color/c_seperator"
        android:layout_height="1dp"/>
    <TextView
        android:id="@+id/tv_add_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d_add_item_tv_height"
        android:text="Add Item"
        android:textSize="@dimen/d_popup_text_size"
        android:textColor="@color/white"
        android:gravity="center"
        />
    <View
        android:layout_width="match_parent"
        android:background="@color/c_seperator"
        android:layout_height="1dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fh_no_of_units"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="# of Units"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_fh_no_of_units"
                android:id="@+id/tv_f_no_of_units"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_f_sq_feet"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Square Feet"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_f_sq_feet"
                android:id="@+id/et_f_sq_feet"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">


        </RelativeLayout>

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:background="@color/c_seperator"
        android:layout_height="1dp"/>

    <TextView
        android:layout_width="match_parent"
        android:text="Gross Schedule Income"
        android:textColor="@color/white"
        android:gravity="center_vertical"
        android:textSize="@dimen/d_popup_text_size"
        android:layout_height="@dimen/d_bdit_input_et" />
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_f_monthly_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Monthly Rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:gravity="center"
                android:layout_marginTop="10dp"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_below="@+id/tv_f_monthly_rent"
                android:id="@+id/tv_f_montly_rent"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_annual_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Annual Rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_below="@+id/tv_annual_rent"
                android:id="@+id/tv_f_annual_rent"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_percentage"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_below="@+id/tv_percentage"
                android:id="@+id/tv_f_percentage"
                android:gravity="center"
                android:text="100%"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_marginTop="10dp"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:background="@color/c_seperator"
        android:layout_height="1dp"/>

    <TextView
        android:layout_width="match_parent"
        android:text="VACANCY LOSS"
        android:textColor="@color/white"
        android:gravity="center_vertical"
        android:textSize="@dimen/d_popup_text_size"
        android:layout_height="@dimen/d_bdit_input_et" />
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"

            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fhvl_sq_feet"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:text="%"
                android:textSize="@dimen/d_cell_txt_size"
                android:background="@color/color_light_grey"
                android:gravity="center"
                android:textColor="@color/blue"
                />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_below="@+id/tv_fhvl_sq_feet"
                android:id="@+id/tv_loss_percentage"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_marginTop="10dp"
                android:background="@drawable/tv_edittext_bg"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fv_monthly_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Monthly Rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_fv_monthly_rent"
                android:id="@+id/tv_fvl_montly_rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fv_annual_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Annual Rent"
                android:textSize="@dimen/d_cell_txt_size"
                android:gravity="center"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:textSize="@dimen/d_cell_txt_size"
                android:gravity="center"
                android:layout_below="@+id/tv_fv_annual_rent"
                android:id="@+id/tv_fvl_annual_rent"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>

    </LinearLayout>




    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:background="@color/c_seperator"
        android:layout_height="1dp"/>

    <TextView
        android:layout_width="match_parent"
        android:text="Other Income"
        android:textColor="@color/white"
        android:gravity="center_vertical"
        android:textSize="@dimen/d_popup_text_size"
        android:layout_height="@dimen/d_bdit_input_et" />
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">


        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fvoi_monthly_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Monthly Rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_fvoi_monthly_rent"
                android:id="@+id/et_foi_montly_rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:background="@drawable/tv_edittext_bg"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fvoi_annual_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Annual Rent"
                android:textSize="@dimen/d_cell_txt_size"
                android:gravity="center"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_below="@+id/tv_fvoi_annual_rent"
                android:id="@+id/tv_foi_annual_rent"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:background="@color/c_seperator"
        android:layout_height="1dp"/>

    <TextView
        android:layout_width="match_parent"
        android:text="Gross Operating Income (Effective Gross Inc)"
        android:textColor="@color/white"
        android:gravity="center_vertical"
        android:textSize="@dimen/d_popup_text_size"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="1dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">


        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fvgoi_monthly_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Monthly Rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_below="@+id/tv_fvgoi_monthly_rent"
                android:id="@+id/tv_fgoi_montly_rent"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tv_fvgoi_annual_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:background="@color/color_light_grey"
                android:text="Annual Rent"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:textColor="@color/blue"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d_add_item_tv_height"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:textSize="@dimen/d_cell_txt_size"
                android:layout_below="@+id/tv_fvgoi_annual_rent"
                android:id="@+id/tv_fgoi_annual_rent"
                android:background="@drawable/tv_disable_bg"
                />

        </RelativeLayout>
    </LinearLayout>

</LinearLayout>